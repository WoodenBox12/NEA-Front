{
  "manifest_version": 3,
  "name": "Computer Science NEA",
  "version": "1.0.0",
  "description": "the web extension for my NEA",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "webRequest",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [ "content-scripts/test.js" ]
    }
  ],
  "icons": {
    "32": "NEA.png"
  }
}